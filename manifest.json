{
    "manifest_version": 2,
    "name": "Tab Intervention",
    "version": "1.0",
    "description": "Limit the number of tabs you can have open",
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
