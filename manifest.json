{
    "manifest_version": 2,
    "name": "Tab Control",
    "version": "1.0",
    "description": "Limit the number of tabs you can have open.",
    "icons": {
        "48": "icons/icon48.png",
        "96": "icons/icon96.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": ["src/background.js"]
    },
    "browser_action": {
        "default_icon": "icons/icon128.png"
    }
}
